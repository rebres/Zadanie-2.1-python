ue = ["Ireland", "Estonia", "Denmark", "Italy", "Slovenia"]
print(ue[3:3]) -> wypisuje pustą kratkę -> []
print(ue[-2]) -> wypisuje wartość przedostatniego elementu -> Italy
print(ue[-6]) -> błąd
print(ue[0:5]) -> wypisuje wszystkie elementy -> ['Ireland', 'Estonia', 'Denmark', 'Italy', 'Slovenia']
print(ue[0:3], ue[3:5]) -> wypisuje dwie kratki z danymi -> ['Ireland', 'Estonia', 'Denmark'] , ['Italy', 'Slovenia']
print(ue[3:-1]) -> wypisuje przedostatnielement -> ['Italy']
print(ue[-2:-4]) -> wypisuje pustą kratkę -> []
print(ue[-4:-2]) -> wypisuje dwa elementy -> ['Estonia', 'Denmark']
